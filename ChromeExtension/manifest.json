{
    "manifest_version": 3,
    "name": "D2-Stats-INATOR",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "/views/home.html",
      "default_icon": "/resorces/images/D2SI-mainIcon.png"
    },
  
    "permissions": [
      "storage",
      "tabs",
      "activeTab",
      "scripting",
      "webRequest",
      "identity"
    ],

  
    "content_scripts": [
      {
        "matches": ["<all_urls>"], 
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "oauth2": {
      "client_id": "41255",
      "scopes":["https://www.bungie.net/en/OAuth/Authorize"]
    },
    "key": ""
    
  }